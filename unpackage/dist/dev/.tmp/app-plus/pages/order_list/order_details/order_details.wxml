<view><view class="nodelivery-detail"><view class="detail-menu"><view class="title">基本信息</view><block wx:if="{{orderDetailInfo.is_have_back}}"><view class="stutas">用户已申请售后</view></block><view class="detail-box"><view data-event-opts="{{[['longpress',[['copy',['$0'],['orderDetailInfo.box_info.order_sn']]]]]}}" class="detail-item" bindlongpress="__e">{{"订单号："+orderDetailInfo.box_info.order_sn}}</view><view data-event-opts="{{[['longpress',[['copy',['$0'],['orderDetailInfo.box_info.delivery_sn']]]]]}}" class="detail-item" bindlongpress="__e">{{"物流单号："+orderDetailInfo.box_info.delivery_sn}}</view><view data-event-opts="{{[['longpress',[['copy',['$0'],['orderDetailInfo.box_info.consignee']]]]]}}" class="detail-item" bindlongpress="__e">{{"收货人："+orderDetailInfo.box_info.consignee}}</view><view data-event-opts="{{[['tap',[['call',['$0'],['orderDetailInfo.box_info.mobile']]]]]}}" class="detail-item " bindtap="__e">手机号：<text class="active">{{''+orderDetailInfo.box_info.mobile+"   点击拨打"}}</text></view><view data-event-opts="{{[['longpress',[['copy',['$0'],['orderDetailInfo.box_info.consignee']]]]]}}" class="detail-item" bindlongpress="__e">{{"所在箱号："+orderDetailInfo.box_info.box_sn}}</view><view class="detail-item">{{"是否取货码通知："+orderDetailInfo.box_info.is_noticed}}</view><view class="detail-item">{{"取货码通知时间："+orderDetailInfo.box_info.order_send_time}}</view><view class="detail-item">{{"到货确认时间："+orderDetailInfo.box_info.order_send_time}}</view></view></view><block wx:if="{{orderDetailInfo.is_have_back==1}}"><view class="detail-menu"><view class="title">售后信息</view><view class="detail-box"><view class="detail-item">{{"售后申请时间："+orderDetailInfo.back_info.add_time}}</view><view class="detail-item">{{"申请角色："+orderDetailInfo.back_info.back_user_type}}</view></view></view></block><view class="detail-menu"><view class="title">商品信息</view><block wx:for="{{orderDetailInfo.goods_info}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="detail-box row"><image class="detail-img" src="{{item.sku_img}}"></image><view class="detail-goods-info"><view class="goods-name">{{item.goods_name}}</view><view class="goods-desc">{{item.spec_key_name}}</view><view class="goods-price">{{"¥"+item.goods_price}}</view><view class="goods-number">{{"x"+item.goods_num}}</view></view></view></block></view><view class="footer-button"><block wx:if="{{orderDetailInfo.is_show_way_button}}"><button data-event-opts="{{[['tap',[['onBack',['$event']]]]]}}" class="back-btn" bindtap="__e">送回仓库</button></block><block wx:if="{{orderDetailInfo.box_info.status==5}}"><button class="back-btn-disabled" disabled>在途中</button></block></view></view></view>