<view><view class="row arrival-header"><input class="arrival-input" maxlength="12" type="text" placeholder="请输入提货箱一维码" data-event-opts="{{[['input',[['arrivalInput',['$event']]]]]}}" value="{{inputValue}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['scanCode',['$event']]]]]}}" class="sys-btn" bindtap="__e">扫码</view></view><block wx:for="{{itemList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="arrival-list"><text class="arrival-time">{{item.box_finish_time}}</text><view class="row-wrap arrival-box"><block wx:for="{{item.box_list}}" wx:for-item="list" wx:for-index="index2" wx:key="index2"><block wx:if="{{list.count>0}}"><view data-event-opts="{{[['tap',[['goArrivalOrder',['$0','$1'],[[['itemList','',index],['box_list','',index2,'status']],[['itemList','',index],['box_list','',index2,'box_sn']]]]]]]}}" class="positionR" bindtap="__e"><block wx:if="{{list.status==2}}"><view class="iconfont icon-xiangbox60 arrival-icon"></view></block><block wx:else><view class="iconfont icon-xiangbox59 arrival-icon icon-color"></view></block><view class="{{['arrival-name '+(list.status==2?'active':'')]}}"><view class="box-name">{{list.pickup_logogram}}</view><view class="box-number">{{list.box_name}}</view></view><view class="arrival-orderNum">{{list.count}}</view></view></block></block></view></view></block></view>