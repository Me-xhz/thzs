<view class="container"><view class="row inputBox"><input class="delivery-input" placeholder="请输入取货码" type="number" focus="true" data-event-opts="{{[['input',[['deliveryInput',['$event']]]]]}}" value="{{inputValue}}" bindinput="__e"/><button data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="btn-sm" bindtap="__e">确认</button></view><view data-event-opts="{{[['tap',[['scanCode',['$event']]]]]}}" class="scan" bindtap="__e">扫一扫</view><popup vue-id="1" isShow="{{isShow}}" text="{{text}}" confirm="确认收货" isFail="{{true}}" cancal="取消" data-event-opts="{{[['^succer',[['succer']]],['^fail',[['fail']]]]}}" bind:succer="__e" bind:fail="__e" bind:__l="__l"></popup></view>